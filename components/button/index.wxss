.at-button {
  position: relative;
  display: flex;
  justify-content: center;
  margin: 0 auto;
  padding: 0 32rpx;
  height: 92rpx;
  color: #333;
  font-size: 32rpx;
  line-height: 92rpx;
  text-align: center;
  border-radius: 8rpx;
  border: 2rpx solid #C5D9E8;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* elements */
  /* modifiers */
}
.at-button--ghost {
  color: #FFF;
  border-color: #FFF;
  background-color: transparent !important;
}
.at-button:active {
  opacity: 0.6;
}
.at-button__icon {
  display: inline-block;
  align-items: center;
  justify-content: center;
  margin: 0 20rpx 0 20rpx;
}
.at-button__text {
  display: inline;
}
.at-button__wxbutton {
  position: absolute;
  padding: 0;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  border: none;
  outline: none;
  background-color: transparent;
  opacity: 0;
  z-index: 1;
}
.at-button__wxbutton::after {
  display: none;
}
.at-button--active {
  opacity: 0.6;
}
.at-button--disabled {
  opacity: 0.3;
}
.at-button--disabled:active {
  opacity: 0.3;
}
.at-button--primary {
  color: #FFF;
  border: 2rpx solid #6190E8;
  background: #6190E8;
}
.at-button--primary--ghost {
  color: #6190E8;
  border-color: #6190E8;
  background-color: transparent !important;
}
.at-button--dashed {
  background-color: #FFF;
  border-style: dashed;
}
.at-button--font {
  border: none;
}
.at-button--danger {
  color: #FFF;
  background-color: #FF4949;
}
.at-button--danger--ghost {
  color: #FF4949;
  border-color: #FF4949;
  background-color: transparent !important;
}
.at-button--small {
  padding: 0 16rpx;
  width: auto;
  height: 60rpx;
  font-size: 28rpx;
  line-height: 60rpx;
}
.at-button--full {
  width: 100%;
  max-width: 100%;
  border-radius: 0;
  border-left: none;
  border-right: none;
}
.at-button--inline {
  display: inline-flex;
  padding: 0 24rpx;
  align-items: center;
  justify-content: center;
  text-align: center;
}
.at-button--inline .at-button__icon {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  margin: 0;
}
.at-button--circle-h5 {
  width: 92rpx;
  height: 92rpx;
  padding: 0;
  font-size: 44rpx;
  border-radius: 50%;
}
.at-button--circle-h5.at-button--small {
  width: 60rpx;
  height: 60rpx;
  padding: 0;
  font-size: 28rpx;
  border-radius: 50%;
}
.at-button--circle {
  width: 92rpx;
  height: 92rpx;
  padding: 0 0 8rpx 0;
  font-size: 44rpx;
  border-radius: 50%;
}
.at-button--circle.at-button--small {
  width: 60rpx;
  height: 60rpx;
  padding: 0 0 4rpx 0;
  font-size: 28rpx;
  border-radius: 50%;
}
.at-button--round {
  opacity: 1;
  height: 92rpx;
  padding: 0 46rpx;
  font-size: 34rpx;
  border-radius: 92rpx;
}
.at-button--round.at-button--small {
  height: 60rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  border-radius: 60rpx;
}